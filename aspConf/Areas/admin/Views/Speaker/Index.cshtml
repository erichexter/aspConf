@model IEnumerable<aspConf.Model.Speaker>
<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            FullName
        </th>
        <th>
            Bio
        </th>
        <th>
            Twitter
        </th>
        <th>
            Email
        </th>
        <th>
            IsActive
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Twitter)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.SpeakerId }) |
            @Html.ActionLink("Details", "Details", new { id=item.SpeakerId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SpeakerId })
        </td>
    </tr>
}

</table>
